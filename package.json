{
  "name": "pokedex-slabs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "dev:api": "pnpm --filter @pokedex-slabs/api dev",
    "dev:web": "pnpm --filter @pokedex-slabs/web dev",
    "dev:worker": "pnpm --filter @pokedex-slabs/worker dev",
    "db:generate": "pnpm --filter @pokedex-slabs/api prisma:generate",
    "db:migrate": "pnpm --filter @pokedex-slabs/api prisma:migrate"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "typescript": "^5.6.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
